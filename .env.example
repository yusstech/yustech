# Environment Variables Template
# Copy this file to .env and fill in the values
# DO NOT commit the actual .env file to version control

# API Keys and Secrets
# Keep these secure and never commit them to version control

# EmailJS Configuration
# Get your keys from: https://dashboard.emailjs.com/admin
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key
VITE_EMAILJS_SERVICE_ID=your_emailjs_service_id
VITE_EMAILJS_TEMPLATE_ID=your_emailjs_template_id

# Cloudflare Turnstile
VITE_CLOUDFLARE_SITE_KEY=your_cloudflare_site_key

# Meta Pixel
VITE_META_PIXEL_ID=your_meta_pixel_id

# Environment
NODE_ENV=development

# Security
VITE_SECURE_COOKIES=true
VITE_CSRF_TOKEN=your_csrf_token 